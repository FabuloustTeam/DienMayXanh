<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testing list product">
	<parameter name="url" value="https://www.dienmayxanh.com/" />
	<listeners>
		<listener class-name="com.dienmayxanh.listener.ListenerTest" />
	</listeners>
	<test name="test view list products">
		<parameter name="SheetName" value="ViewListProducts" />
		<classes>
			<class name="com.dienmayxanh.test.ViewListProducts" />
		</classes>
	</test>

	<test name="test comment">
		<parameter name="SheetName" value="Comment" />
		<classes>
			<class name="com.dienmayxanh.test.Comment" />
		</classes>
	</test>
</suite>